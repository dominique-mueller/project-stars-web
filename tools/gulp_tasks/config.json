{
	"paths": {
		"project": {
			"src": "./public",
			"dest": "./build"
		},
		"app": {
			"src": "./public/app",
			"dest": "./build/app",
			"vendor": "./build/vendor"
		},
		"styles": {
			"src": "./public/styles",
			"dest": "./build/assets"
		},
		"icons": {
			"src": "./public/assets/icons"
		},
		"documentation": {
			"base": "./docs",
			"frontend": "./docs/frontend"
		},
		"typings": {
			"default": "./typings"
		}
	},
	"names": {
		"styles": "style.min.css"
	}
}
